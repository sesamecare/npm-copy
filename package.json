{
  "name": "@sesamecare-oss/npm-copy",
  "version": "1.0.0",
  "description": "A utility to clone packages from one NPM server to another",
  "author": "Sesame <developers@sesamecare.com>",
  "contributors": [
    "Bob Zoller <bob@zoller.us>",
    "Max Metral <developers@pyralis.com>"
  ],
  "license": "MIT",
  "keywords": [
    "npm"
  ],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "bin": "./build/bin/npm-copy.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/sesamecare/npm-copy.git"
  },
  "homepage": "https://github.com/sesamecare/npm-copy",
  "bugs": "https://github.com/sesamecare/npm-copy/issues",
  "config": {
    "coconfig": "@openapi-typescript-infra/coconfig"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@openapi-typescript-infra/coconfig": "^3.0.2",
    "@types/debug": "^4.1.8",
    "@types/jest": "^29.5.2",
    "@types/libnpmpublish": "^4.0.3",
    "@types/lodash.difference": "^4.5.7",
    "@types/lodash.omitby": "^4.6.7",
    "@types/minimist": "^1.2.2",
    "@types/node": "^20.3.1",
    "@types/npm-registry-fetch": "^8.0.4",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "coconfig": "^0.12.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "pinst": "^3.0.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">16.0.0"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc --skipLibCheck -p tsconfig.build.json && yarn dlx glob-chmod 755 build/bin/*",
    "clean": "yarn dlx rimraf ./build",
    "lint": "eslint src",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "postinstall": "husky install && coconfig"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "yarn eslint --cache --fix"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "packageManager": "yarn@3.6.0",
  "dependencies": {
    "debug": "^4.3.4",
    "libnpmpublish": "^7.3.0",
    "lodash.difference": "^4.5.0",
    "lodash.omitby": "^4.6.0",
    "minimist": "^1.2.8",
    "npm-registry-fetch": "^14.0.5"
  }
}
